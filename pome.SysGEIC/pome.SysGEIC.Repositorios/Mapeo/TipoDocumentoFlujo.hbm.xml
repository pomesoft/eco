<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="pome.SysGEIC.Entidades" namespace="pome.SysGEIC.Entidades" xmlns="urn:nhibernate-mapping-2.2">
    <class name="pome.SysGEIC.Entidades.TipoDocumentoFlujo" table="TipoDocumentoFlujos" lazy="false">
        <id name="Id" column="IdFlujo">
            <generator class="identity" />
        </id>
        <many-to-one name="TipoDocumento" column="IdTipoDocumento" class="pome.SysGEIC.Entidades.TipoDocumento" />
        <property name="Descripcion" column="Descripcion" />
        <property name="Vigente" column="Vigente" />
        <bag name="Estados" inverse="true" cascade="all-delete-orphan">
            <key column="IdFlujo" />
            <one-to-many class="pome.SysGEIC.Entidades.TipoDocumentoFlujoEstado" />
        </bag>
    </class>
</hibernate-mapping>