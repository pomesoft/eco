<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="pome.SysGEIC.Entidades" namespace="pome.SysGEIC.Entidades" xmlns="urn:nhibernate-mapping-2.2">
    <class name="pome.SysGEIC.Entidades.Acta" table="Actas" lazy="false">
        <id name="Id" column="IdActa">
            <generator class="identity" />
        </id>
        <property name="Descripcion" column="Descripcion" />
        <property name="Vigente" column="Vigente" />
        <property name="Fecha" column="Fecha" />
        <property name="Hora" column="Hora" />
        <bag name="Participantes" inverse="true" cascade="all-delete-orphan">
            <key column="IdActa" />
            <one-to-many class="pome.SysGEIC.Entidades.ActaProfesional"/>
        </bag>
        <bag name="Estudios" inverse="true" cascade="all-delete-orphan" order-by="OrdenEstudio">
            <key column="IdActa" />
            <one-to-many class="pome.SysGEIC.Entidades.ActaEstudio" />
        </bag>
        <bag name="Documentos" inverse="true" cascade="all-delete-orphan" order-by="OrdenEstudio, OrdenDocumento">
            <key column="IdActa" />
            <one-to-many class="pome.SysGEIC.Entidades.ActaDocumento" />
        </bag>
        <property name="ComentarioInicialFijo" column="ComentarioInicialFijo" />
        <property name="ComentarioInicial" type="StringClob">
            <column name="ComentarioInicial" sql-type="ntext" not-null="false" />
        </property>
        <property name="ComentarioFinal" type="StringClob">
            <column name="ComentarioFinal" sql-type="ntext" not-null="false" />
        </property>
        <property name="Cerrada" column="Cerrada" />
    </class>
</hibernate-mapping>