<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="pome.SysGEIC.Entidades" namespace="pome.SysGEIC.Entidades" xmlns="urn:nhibernate-mapping-2.2">
    <class name="pome.SysGEIC.Entidades.ActaDocumento" table="ActaDocumentos" lazy="false">
        <id name="Id" column="IdActaDocumento">
            <generator class="identity" />
        </id>        
        <many-to-one name="Acta" column="IdActa" class="pome.SysGEIC.Entidades.Acta" />
        <many-to-one name="DocumentoVersion" column="IdDocumentoVersion" class="pome.SysGEIC.Entidades.DocumentoVersion" />
        <many-to-one name="ResponsableComite" column="IdProfesional" class="pome.SysGEIC.Entidades.Profesional" />
        <!--<property name="Descripcion" column="Comentario" />-->
        <property name="Descripcion" type="StringClob">
            <column name="Comentario" sql-type="ntext" not-null="false" />
        </property>
        <property name="OrdenEstudio" column="OrdenEstudio" />
        <property name="OrdenDocumento" column="OrdenDocumento" />
        <property name="ImprimirCarta" column="ImprimirCarta" />
        <property name="SetearSaltosLinea" column="SetearSaltosLinea" />
    </class>
</hibernate-mapping>
