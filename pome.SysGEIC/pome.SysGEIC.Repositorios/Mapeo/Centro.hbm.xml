<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="pome.SysGEIC.Entidades" namespace="pome.SysGEIC.Entidades" xmlns="urn:nhibernate-mapping-2.2">
    <class name="pome.SysGEIC.Entidades.Centro" table="Centros" lazy="false">
        <id name="Id" column="IdCentro">
            <generator class="identity" />
        </id>
        <property name="Descripcion" column="RazonSocial" />
        <property name="Vigente" column="Vigente" />
        <property name="Direccion" column="Direccion" />
        <property name="Telefono" column="Telefono" />
        <property name="Email" column="Email" />
        <property name="IdCentroVinculado" column="IdCentroVinculado" />
    </class>
</hibernate-mapping>