<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="pome.SysGEIC.Entidades" namespace="pome.SysGEIC.Entidades" xmlns="urn:nhibernate-mapping-2.2">
    <class name="pome.SysGEIC.Entidades.RecordatorioDocumento" table="RecordatorioDocumentos" lazy="false">
        <id name="Id" column="IdRecordatorioDocumento">
            <generator class="identity" />
        </id>
        <many-to-one name="Recordatorio" column="IdRecordatorio" class="pome.SysGEIC.Entidades.Recordatorio" />
        <many-to-one name="Documento" column="IdDocumento" class="pome.SysGEIC.Entidades.Documento" />
        <property name="Meses" column="Meses" />
    </class>
</hibernate-mapping>
