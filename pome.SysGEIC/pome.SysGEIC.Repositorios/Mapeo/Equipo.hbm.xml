<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="pome.SysGEIC.Entidades" namespace="pome.SysGEIC.Entidades" xmlns="urn:nhibernate-mapping-2.2">
    <class name="pome.SysGEIC.Entidades.Equipo" table="Equipos" lazy="false">
        <id name="Id" column="IdEquipo">
            <generator class="identity" />
        </id>
        <property name="Descripcion" column="Descripcion" />
        <property name="Vigente" column="Vigente" />
        <bag name="Integrantes" inverse="true" cascade="all-delete-orphan">
            <key column="IdEquipo" />
            <one-to-many class="pome.SysGEIC.Entidades.IntegranteEquipo" />
        </bag>
    </class>
</hibernate-mapping>
